public class Main {

    public static void main(String[] args) {
        int fibSum = 0;
        int fibNum1 = 1;
        int fibNumTemp = 0;
        for (int i = 1; i <= 4000000; i = i + fibNumTemp){
            fibNumTemp = fibNum1;
            fibNum1 = i;
            if (i % 2 == 0){
                fibSum = fibSum + i;
            }
        }
        System.out.println(fibSum);
    }
}
