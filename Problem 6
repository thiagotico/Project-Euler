public class Main {

    public static void main(String[] args) {
        int numSum = 0;
        int numSquareSum = 0;
        for (int i = 1; i <= 100; i++){
            numSum = numSum + i;
        }
        numSum = numSum * numSum;
        for (int i = 1; i <= 100; i++){
            numSquareSum = numSquareSum + i * i;
        }
        System.out.println(numSquareSum - numSum);
    }
}
